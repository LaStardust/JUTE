{
  "name": "jute",
  "description": "Javascript Unit Test Environment",
  "keywords": ["selenium", "test", "testing", "unit", "tests"],
  "version": "0.0.13",
  "author": "Mark Ethan Trostler <mark@zzo.com>",
  "preferGlobal": true,
  "bin" : { 
    "jute" :            "./jute_backend.js",
    "jute_submit_test": "./submit_test.js",
    "jute_v8":          "./jute_v8.js"
  },
  "scripts" : { 
    "start" : "node ./jute_backend.js start",
    "stop"  : "node ./jute_backend.js stop"
  },
  "engines": { "node": ">= 0.4.0" },
  "repository": {
    "type": "git",
    "url": "git://github.com/zzo/JUTE.git"
  },
  "homepage": "https://github.com/zzo/JUTE",
  "dependencies" :
    {
        "soda" : ">= 0.2.4"
      , "node-uuid": ">= 1.2.0"
      , "glob" : ">= 2.0.7"
      , "connect" : ">= 1.6.0"
      , "yui3": ">= 0.5.33"
      , "xmlhttprequest" : ">= 1.2.2"
      , "cookie-sessions" : ""
      , "jsdom": ""
      , "mime" : ""
      , "daemon" : ""
      , "optimist" : ""
  },
  "config" : {
    "port" : "8080",
    "docRoot":  "/var/www/",
    "testDir":  "test/",
    "outputDir": "output/",
    "java": "/usr/bin/java",
    "logFile": "/tmp/jute.log",
    "testRegex": "*.htm*"
  }
}

