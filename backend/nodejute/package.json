{
  "name": "jute",
  "description": "Javascript Unit Test Environment",
  "keywords": ["selenium", "test", "testing", "unit", "tests"],
  "version": "0.0.57",
  "author": "Mark Ethan Trostler <mark@zzo.com>",
  "preferGlobal": true,
  "bin" : { 
    "jute" :            "./jute_backend.js",
    "jute_submit_test": "./submit_test.js",
    "jute_v8":          "./jute_v8.js"
  },
  "scripts" : {
    "start"   : "node ./jute/daemon.js --start",
    "stop"    : "node ./jute/daemon.js --stop"
  },
  "engines": { "node": "~0.4" },
  "repository": {
    "type": "git",
    "url": "git://github.com/zzo/JUTE.git"
  },
  "homepage": "https://github.com/zzo/JUTE",
  "dependencies" :
    {
        "soda" : "~0.2"
      , "node-uuid": "~1.2"
      , "connect" : "~1.7"
      , "npm" : "~1.0"
      , "yui3": "~0.7"
      , "xmlhttprequest" : "~1.2"
      , "cookie-sessions" : "~0.0"
      , "mime" : "~1.2"
      , "optimist" : "~0.2"
  },
  "config" : {
    "port" : "5883",
    "docRoot":  "/var/www/",
    "testDir":  "test/",
    "outputDir": "output/",
    "java": "/usr/bin/java",
    "logfile": "/tmp/jute.log",
    "pidfile": "/tmp/jute.pid",
    "inject": "1",
    "testRegex": "*.htm*"
  }
}

